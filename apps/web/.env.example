# Backend API base URL (browser)
NEXT_PUBLIC_API_URL=http://localhost:4000

# Sui RPC (browser)
NEXT_PUBLIC_SUI_RPC_URL=https://fullnode.testnet.sui.io

# Deployed Move package ID (browser)
NEXT_PUBLIC_PACKAGE_ID=

# zkLogin Configuration (browser)
# Get these from Google Cloud Console: https://console.cloud.google.com/
NEXT_PUBLIC_GOOGLE_CLIENT_ID=
NEXT_PUBLIC_REDIRECT_URI=http://localhost:3000/auth/callback

# zkLogin prover service (Mysten Labs - default works for testnet)
NEXT_PUBLIC_PROVER_URL=https://prover-dev.mystenlabs.com/v1

# Salt secret seed - combined with Google sub to derive user's salt
# IMPORTANT: Keep consistent! Changing this changes all user addresses!
ZKLOGIN_USER_SALT=your-secret-salt-seed-here

# ENS: set to true to use vitalik.eth for "Connect EVM wallet & load ENS" (no wallet required)
# NEXT_PUBLIC_ENS_TEST_MODE=false

# Yellow Network: sandbox mode (Sepolia + ytest.usd)
# NEXT_PUBLIC_YELLOW_SANDBOX=true
